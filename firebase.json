{
  "hosting": {
    "public": "_site",
    "headers": [
      {
        "source" : "**/*.@(jpg|jpeg|gif|png|webp)",
        "headers" : [ {
          "key" : "Cache-Control",
          "value" : "max-age=15778800"
        } ]
      }, {
        "source" : "**/*.@(js|css)",
        "headers" : [ {
          "key" : "Cache-Control",
          "value" : "max-age=3600"
        } ]
      }, {
        "source" : "404.html",
        "headers" : [ {
          "key" : "Cache-Control",
          "value" : "max-age=300"
        } ]
      }
    ]
  }
}
