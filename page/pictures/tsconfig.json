{
  "compilerOptions": {
    "target": "es2015",
    "lib": ["DOM", "esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "baseUrl": ".",
    "strict": true,
    "noEmitHelpers": false,
    "noUnusedLocals": false,
    "removeComments": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true
  }
}
